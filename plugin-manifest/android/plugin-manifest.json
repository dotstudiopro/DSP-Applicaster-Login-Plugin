{
  "api": {
    "require_startup_execution": true,
    "class_name": "com.dotstudioz.LoginPlugin"
  },
  "dependency_repository_url": [
    {
      "url": "https://jitpack.io"
    }
  ],
  "dependency_name": "com.github.dotstudiopro:DSP-Applicaster-Login-Plugin",
  "dependency_version": "0.0.27",
  "platform": "android",
  "manifest_version": "0.1.2",
  "author_email": "mohsin@dotstudioz.com",
  "author_name": "Mohsin Shaikh",
  "name": "DotstudioLogin",
  "description": "Dotstudio Login plugin for applicaster",
  "type": "login",
  "screen": true,
  "identifier": "com.dotstudioz.loginplugin",
  "ui_builder_support": true,
  "whitelisted_account_ids": [
    "5d812bbb7b81ea000e1168de"
  ],
  "min_zapp_sdk": "11.3.1",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": false,
  "preload": true,
  "custom_configuration_fields": [
    {
      "type": "text",
      "key": "Title",
      "label": "Title",
      "tooltip_text": "Enter Company Title.",
      "default": "Dotstudio"
    },
    {
      "type": "text",
      "key": "apiKey",
      "tooltip_text": "Enter Dotstudio Company api key.",
      "default": ""
    },
    {
      "type": "text",
      "key": "auth0ClientId",
      "tooltip_text": "Enter Auth0 client ID.",
      "default": ""
    },
    {
      "type": "color_picker",
      "key": "backgroundColor",
      "label": "Background Color",
      "initial_value": "#00FF00FF",
      "tooltip_text": "Select Background color."
    },
    {
      "type": "color_picker",
      "key": "headerColor",
      "initial_value": "#00FF00FF",
      "tooltip_text": "Select Header color."
    },
    {
      "type": "color_picker",
      "key": "titleColor",
      "initial_value": "#FF00FFFF",
      "tooltip_text": "Select Title color."
    },
    {
      "type": "uploader",
      "key": "logo",
      "tooltip_text": "Upload Your company Logo."
    },
    {
      "type": "uploader",
      "key": "assets_bundle",
      "tooltip_text": "Upload a Zip file following the folder structure and hierarchy guideline of the platform. The Zip file will be extracted during the build time and will add the assets to the resources of the bundle/package. This field does not affect runtime and a change will require a new build of the app version."
    },
    {
      "key": "show_on_startup",
      "type": "switch",
      "initial_value": true,
      "label": "Show on Start ?",
      "tooltip_text": "Turn this off if You don't want to show login screen on start."
    },
    {
      "type": "text",
      "key": "visitWebsiteMessage",
      "tooltip_text": "Visit website to subscribe!!!",
      "default": "Visit website to subscribe!!!"
    }
  ],
  "hooks": {
    "fields": [
      {
        "group": true,
        "label": "Before Load",
        "folded": true,
        "fields": [
          {
            "key": "preload_plugins",
            "type": "preload_plugins_selector",
            "label": "Select Plugins"
          }
        ]
      }
    ]
  }
}